#!/bin/bash

echo ""
echo "Applying migration ComingSoon"

echo "Adding routes to conf/app.routes"
echo "" >> ../conf/app.routes
echo "GET        /comingSoon                       controllers.ComingSoonController.onPageLoad()" >> ../conf/app.routes

echo "Adding messages to conf.messages"
echo "" >> ../conf/messages.en
echo "comingSoon.title = comingSoon" >> ../conf/messages.en
echo "comingSoon.heading = comingSoon" >> ../conf/messages.en

echo "Migration ComingSoon completed"
